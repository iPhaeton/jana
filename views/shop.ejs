
<!DOCTYPE html>
<html>
    <head>
        <title><%= title %></title>

        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        <link rel='stylesheet' href="/build/common.css"/>
        <link rel='stylesheet' href="/build/shop.css"/>

    </head>

    <body>

        <div class="header">
            
            <div class="container">
                <%- partial("../public/components/headMenu/headMenu.ejs")%>
            </div>
            
        </div>

        <div class="container main-content" admin-mode="<%= user ? user.admin : false %>">
            <div class="row">
                <%- partial("../public/components/sideMenu/sideMenu.ejs")%>
                <div class="col-sm-10">
                    <div id="commodity-list" class="thumbnails">

                    </div>
                </div>
            </div>
            <div class="edit-panel" hidden="true">
                <button type='button' class='btn btn-default edit-button' id="add-commodity">Добавить товар</button>
                <button type='button' class='btn btn-default edit-button' id="add-category">Добавить категрию</button>
                <div class="clearfix"></div>
            </div>
        </div>

        <div class="clearfix"></div>

        <script src="/build/common.js"></script>
        <script src="/build/shop.js"></script>

    </body>
</html>
