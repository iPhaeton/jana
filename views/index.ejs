<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>

    <link rel="stylesheet" href="/vendor/bower_components/bootstrap/dist/css/bootstrap.css"/>
    <link rel='stylesheet' href='/stylesheets/style.css' />

    <script src="/vendor/bower_components/jquery/dist/jquery.js"></script>
    <script src="/vendor/bower_components/bootstrap/dist/js/bootstrap.js"></script>
  </head>

  <body>

    <div class="header">
      <div class="container">

        <div class="head-menu" role="navigation">

          <ul class="nav nav-pills head-menu__left head-menu__panel">
            <% for (var menuTitle in headMenuLeft) {%>
              <li><a href=<%= headMenuLeft[menuTitle] %> role="presentation"><%= menuTitle %></a></li>
            <% } %>
          </ul>

          <% if (headMenuRight) { %>
            <ul class="nav nav-pills head-menu__right head-menu__panel">
              <% for (var menuTitle in headMenuRight) {%>
                <li><a href=<%= headMenuRight[menuTitle] %> role="presentation"><%= menuTitle %></a></li>
              <% }; %>
            </ul>
          <% } %>

        </div>

      </div>
    </div>

    <div class="jumbotron showcase">
      <div class="showcase__image">
        <img src="/images/showcase.jpg">
      </div>
    </div>


  </body>
</html>
